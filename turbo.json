{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": [
		"**/.env.*local"
	],
	"pipeline": {
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				".next/**",
				"!.next/cache/**"
			]
		},
		"lint": {},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"db:reset": {
			"cache": false,
			"persistent": false
		},
		"db:migrate": {
			"cache": false,
			"persistent": false
		}
	}
}
